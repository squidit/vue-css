{
  "name": "css-vue",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rm -rf src/dist && vue-tsc && vite build src",
    "postinstall": "npx husky install && cd src && npm install",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "reset": "rm package-lock.json && rm -rf node_modules/@squidit/css-vue && npm install",
    "start": "storybook dev -p 6006",
    "start:application": "vite",
    "test": "jest src/src/components",
    "update": "rm -rf src/dist && vite build src && npm run reset"
  },
  "dependencies": {
    "@squidit/css-vue": "file:./src",
    "src": "^1.0.0",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/vue3": "^7.5.3",
    "@storybook/vue3-vite": "^7.5.3",
    "@types/jest": "^29.5.8",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.2",
    "eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "jest": "^26.3.3",
    "sass": "^1.69.5",
    "storybook": "^7.5.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.9.5",
    "vite": "^5.0.0",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^1.8.22"
  }
}
