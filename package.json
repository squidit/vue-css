{
  "name": "@squidit/vue-css",
  "version": "1.3.1",
  "scripts": {
    "start": "vite",
    "storybook": "storybook dev -p 6006",
    "build": "rm -rf dist && vue-tsc && vite build",
    "build-storybook": "vue-tsc && storybook build",
    "deploy-storybook": "npm run build-storybook && touch ./storybook-static/.nojekyll",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.vue --fix",
    "prepare": "husky install"
  },
  "description": "Vue CSS is an abstraction in Vue3 of the Squid CSS framework",
  "type": "module",
  "main": "dist/vue-css.cjs.js",
  "module": "dist/vue-css.es.js",
  "types": "dist/vue-css.d.ts",
  "files": [
    "/dist"
  ],
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/squidit/vue-css.git"
  },
  "homepage": "https://vue-css.squidit.com.br",
  "keywords": [
