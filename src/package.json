    "@vitejs/plugin-vue": "^4.4.1",
    "path": "^0.12.7",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.3",
    "vue-tsc": "^1.8.22"
